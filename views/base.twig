<!DOCTYPE html>
<html {{ site.language_attributes }} >
  <head>
    <meta charset="{{ site.charset }}">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="https://kit.fontawesome.com/88601afd06.js" crossorigin="anonymous"></script>
    {% block extrahead %}{% endblock %}
    {{function('wp_head')}}
  </head>
  <body class="{{body_class}}">
    {% import '_macros.twig' as JRSP %}
    {# main content #}
    <div class="container">
    {% block content %}
    {% endblock %} 
    </div>
    {# Footer #}     
    {% include 'snippets/footer.twig' %}
    {{function('wp_footer')}}
    <!--[if IE 9]>
    <script src="https://raw.githubusercontent.com/jonathantneal/svg4everybody/master/dist/svg4everybody.min.js"></script>
    <script>svg4everybody();</script>
    <![endif]-->
    {% block extrajs %}{% endblock %}
  </body>
</html>
